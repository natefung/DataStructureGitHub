
@{
    ViewBag.Title = "Index";
}

<h2>Dictionary</h2>

<div class="w3-bar w3-border w3-light-grey">
    <a href="@Url.Action("AddOne", "Dictionary")" class="w3-bar-item w3-button">1. Add one time to Dictionary</a>
    <br />
    <a href="@Url.Action("AddHuge", "Dictionary")" class="w3-bar-item w3-button w3-hover-green">2. Add Huge List of Items  to Dictionary</a>
    <br />
    <a href="@Url.Action("DisplayDictionary", "Dictionary")" class="w3-bar-item w3-button w3-hover-blue">3. Display Dictionary</a>
    <br />
    <a href="@Url.Action("DeleteFrom", "Dictionary")" class="w3-bar-item w3-button w3-hover-teal">4. Delete from Dictionary</a>
    <br />
    <a href="@Url.Action("ClearDictionary", "Dictionary")" class="w3-bar-item w3-button w3-hover-teal">5. Clear Dictionary</a>
    <br />
    <a href="@Url.Action("SearchDictionary", "Dictionary")" class="w3-bar-item w3-button w3-hover-teal">6. Search Dictionary</a>
    <br />
    <a href="@Url.Action("Index", "Index")" class="w3-bar-item w3-button w3-hover-teal">7. Return to Main Menu</a>
</div>

<div align="center" position="top">
    <p>@ViewBag.Delete</p>
    <p>@ViewBag.SearchResults</p>
</div>
